@model ImageGallery.Client.ViewModels.DumpTokenModel

@{
    ViewData["Title"] = "View";
}

<h2>View</h2>

<div>
    <h4>DumpTokenModel</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.AccessToken)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AccessToken)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.IdToken)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdToken)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.RefreshToken)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.RefreshToken)
        </dd>
    </dl>
    <hr />
    <h4>Claims of current User</h4>
    <dl class="dl-horizontal">
        @foreach (var claim in Model.Claims)
        {
            <dt>
                @claim.Type
            </dt>
            <dd>
                @claim.Value
            </dd>
        }
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
